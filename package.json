{
  "name": "dag-progress",
  "version": "1.0.2",
  "description": "Calculate user progress for each vertex in an acyclic digraph",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joedski/dag-progress.git"
  },
  "bugs": {
    "url": "https://github.com/joedski/dag-progress/issues"
  },
  "scripts": {
    "clean": "rimraf lib es",
    "build": "npm run build:commonjs && npm run build:es",
    "build:es": "cross-env BABEL_ENV=es babel source --out-dir es",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel source --out-dir lib",
    "prepublish": "npm run clean && npm run build",
    "preversion": "npm run clean && npm run test",
    "version": "npm run build",
    "test": "ava"
  },
  "keywords": [
    "dag",
    "directed",
    "acyclic",
    "digraph",
    "graph",
    "progress",
    "story",
    "interaction"
  ],
  "author": "Joseph Sikorski <joedski@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "fraction.js": "^3.3.1",
    "toposort": "^0.2.12"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.7.2",
    "cross-env": "^1.0.7",
    "eslint-plugin-flow-vars": "^0.3.0",
    "rimraf": "^2.5.2"
  },
  "ava": {
    "babel": "inherit",
    "files": [
      "test/*.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-flow-strip-types"
    ]
  }
}
